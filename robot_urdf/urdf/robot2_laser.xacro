		<child link="arm_3"/>
		<axis xyz="1 0  0"/>
		<origin xyz="0.15 0  0.25"/>
		<limit effort ="100.0" lower="-0.9" upper="2" velocity="0.5"/>

</joint>
	




 <link name="arm_4">
  	<inertial>
  		<mass value="0.028"/>
  		<origin xyz="0 0 0" rpy="0 0 0" />
