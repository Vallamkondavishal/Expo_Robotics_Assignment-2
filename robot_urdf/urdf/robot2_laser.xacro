        rpy="1.5707963267949 0 0" />
      <geometry>
        <cylinder length="${length_wheel}" radius="${radius_wheel}"/>
      </geometry>
    </collision>
  </link>

<link name="link_front_left_wheel">
    <inertial>
      <origin
        xyz="-4.1867E-18 0.0068085 -1.65658661799998E-18"
        rpy="0 0 0" />
      <mass
        value="0.5" />
      <inertia
        ixx="0.00856502765719703"
        ixy="1.5074118157338E-19"
        ixz="-4.78150098725052E-19"
        iyy="0.013670640432096"
        iyz="-2.68136447099727E-19"
        izz="0.00856502765719703" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="1.5707963267949 0 0" />
      <geometry>
        <mesh filename="package://robot_urdf/meshes/wheel.stl" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="1.5707963267949 0 0" />
      <geometry>
        <cylinder length="${length_wheel}" radius="${radius_wheel}"/>
      </geometry>
    </collision>
  </link> 


  <joint name="joint_back_right_wheel" type="continuous">
    <origin rpy="0 0 3.14" xyz="-0.15 0.26 0"/>
    <child link="link_back_right_wheel"/>
    <parent link="link_chassis"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>
  
  
  <joint name="joint_back_left_wheel" type="continuous">
    <origin rpy="0 0 0" xyz="-0.15 -0.26 0"/>
    <child link="link_back_left_wheel"/>
    <parent link="link_chassis"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>

  <joint name="joint_front_right_wheel" type="continuous">
    <origin rpy="0 0 3.14" xyz="0.15 0.26 0"/>
    <child link="link_front_right_wheel"/>
    <parent link="link_chassis"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>
  
  
  <joint name="joint_front_left_wheel" type="continuous">
    <origin rpy="0 0 0" xyz="0.15 -0.26 0"/>
    <child link="link_front_left_wheel"/>
    <parent link="link_chassis"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint> 
  
  
<!--############################## HOKUYO_LINK ########################################-->


  
   <link name="hokuyo_link">
	    <collision>
	      <origin xyz="0 0 0.025" rpy="0 0 0"/>
	      <geometry>
		<box size="0.05 0.09 0.05"/>
	      </geometry>
	    </collision>
	    <visual>
	      <origin xyz="0 0 0.025" rpy="0 0 0"/>
	      <geometry>
		<box size="0.05 0.09 0.05"/>
	      </geometry>
	      <material name="red">
		<color rgba="1 0 0 1"/>
	      </material>
	    </visual>
	    <inertial>
	      <mass value="1e-5" />
	      <origin xyz="0 0 0" rpy="0 0 0"/>
	      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
	    </inertial> 
 	 </link>
 	 
 	 <joint name="hokuyo_joint" type="fixed">
	    <axis xyz="0 1 0" />
	    <origin xyz="0.35 0 0.035" rpy="0 0 0"/>
	    <parent link="link_chassis"/>
	    <child link="hokuyo_link"/> 
	 </joint>


<!--############################## ARM ########################################-->

 <link name="arm_base">
  	<inertial>
  		<mass value="0.5"/>
  		<origin xyz="7.7128E-09 -0.063005 -3.01969999961422E-08" rpy="0 0 0"  />
  		
  		<inertia ixx="0.0000416666667" ixy="0" ixz="0" iyy="0.0000416666667" iyz="0" izz="0.0000416666667"/>
  	</inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 1.5707963267949" />
      <geometry>
	       <mesh filename="package://robot_urdf/meshes/arm_base.stl" />
      </geometry>
    </visual>
    
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 1.5707963267949" />
      <geometry>
	        <mesh filename="package://robot_urdf/meshes/arm_base.stl" />
      </geometry>
    </collision>
</link> 

  <joint name="base_to_arm_base" type="continuous">
                <limit effort="10000" velocity="1000"/>
                <joint_properties damping="1.0" friction="1.0"/>
		<parent link="link_chassis"/>
		<child link="arm_base"/>
		<axis xyz="0 0 1"/>
		<origin xyz="0 0 0.150"/>
  </joint>


  <link name="arm_1">
  	<inertial>
  		<mass value="0.1"/>
  		<origin xyz="0 0 0" rpy="0 0 0" />
  		<inertia ixx="0.000010279" ixy="0" ixz="0" iyy="0.000010279" iyz="0" izz="0.000007225"/>
  	</inertial>
  	<collision>
  	  <origin xyz="0 0 0.25" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.5" radius="0.03"/>
      </geometry>
  	</collision>
  	<visual>
  	  <origin xyz="0 0 0.25" rpy="0 0 0"/>
      <geometry>
       <cylinder length="0.5" radius="0.03"/>
      </geometry>
      <material name="red" />
  	</visual>
  </link>

  <joint name="arm_1_to_arm_base" type="revolute">
		<parent link="arm_base"/>
		<child link="arm_1"/>
		<axis xyz="0 1 0"/>
		<origin xyz="0 0.0 0.25"/>
		<limit effort ="100.0" lower="-1.5707963267949" upper="1.5707963267949" velocity="0.5"/>
 </joint>
 
 
      <link name="arm_2">
  	<inertial>
  		<mass value="0.02"/>
  		<origin xyz="0 0 0" rpy="0 0 0" />
  		<inertia ixx="0.000010279" ixy="0" ixz="0" iyy="0.000010279" iyz="0" izz="0.000007225"/>
  	</inertial>
  	<collision>
  	  <origin xyz="0.06 0 0.15" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.5" radius="0.03"/>
      </geometry>
  	</collision>
  	<visual>
  	  <origin xyz="0.06 0 0.15" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.5" radius="0.03"/>
      </geometry>
      <material name="red" />
  	</visual>
  </link>

   <joint name="arm_2_to_arm_1" type="revolute">
		<parent link="arm_1"/>
		<child link="arm_2"/>
		<axis xyz="0 2 0"/>
		<origin xyz="-0.07 -0.06 0.50"/>
		<limit effort ="100.0" lower="-1.5707963267949" upper="1.5707963267949" velocity="0.5"/>
	</joint>




	
  <link name="arm_3">
  	<inertial>
  		<mass value="0.015"/>
  		<origin xyz="0 0 0" rpy="0 0 0" />
  		<inertia ixx="0.000010200" ixy="0" ixz="0" iyy="0.000010200" iyz="0" izz="0.000006225"/>
  	</inertial>
  	<collision>
  	  <origin xyz="0.06 0 0.15" rpy="0 0 1.5"/>
      <geometry>
        <box size="0.06 0.6 0.06"/>
      </geometry>
  	</collision>
  	<visual>
