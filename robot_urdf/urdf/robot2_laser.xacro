<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="robot2_laser_xacro">
<xacro:include filename="$(find robot_urdf)/urdf/robot2_laser.gazebo"/>
					
	<xacro:property name="length_wheel" value="0" />
	<xacro:property name="radius_wheel" value="0" />

	<xacro:macro name="default_inertial" params="mass">
               <inertial>
                       <mass value="${mass}" />
                       <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
                       <inertia ixx="0.000526666666667" ixy="0" ixz="0" iyy="0.000526666666667" iyz="0" izz="0.001"/>
               </inertial>
	</xacro:macro>

    <xacro:macro name="wheel_geometry">
		<geometry>
       			<cylinder length="${length_wheel}" radius="${radius_wheel}"/>
     	</geometry>
	</xacro:macro>
	
<!--################################# COLOUR #####################################-->

	
  <material name="black">
    <color rgba="0.0 0.0 0.0 1.0"/>
  </material>
  <material name="blue">
    <color rgba="0.203125 0.23828125 0.28515625 1.0"/>
  </material>
  <material name="green">
    <color rgba="0.0 0.8 0.0 1.0"/>
  </material>
  <material name="grey">
    <color rgba="0.2 0.2 0.2 1.0"/>
  </material>
  <material name="orange">
    <color rgba="1.0 0.423529411765 0.0392156862745 1.0"/>
  </material>
  <material name="brown">
    <color rgba="0.870588235294 0.811764705882 0.764705882353 1.0"/>
  </material>
  <material name="red">
    <color rgba="0.80078125 0.12890625 0.1328125 1.0"/>
  </material>
  <material name="white">
    <color rgba="1.0 1.0 1.0 1.0"/>
  </material>
  
<!--################################# BASE #####################################-->  

  <link name="link_chassis">
    <!-- pose and inertial -->
    <inertial>
      <mass value="10"/>
      <origin rpy="0 0 0" xyz="0 0 0.1"/>
      <inertia ixx="0.0395416666667" ixy="0" ixz="0" iyy="0.106208333333" iyz="0" izz="0.106208333333"/>
    </inertial>
    
    <!-- body -->
    <collision name="collision_chassis">
      <origin
        xyz="0 0 0"
        rpy="1.5707963267949 0 3.14" />
      <geometry>
        <mesh filename="package://robot_urdf/meshes/robot_base.stl" />
      </geometry>
    </collision>
    
    <visual>
      <origin
        xyz="0 0 0"
        rpy="1.5707963267949 0 3.14" />
      <geometry>
        <mesh filename="package://robot_urdf/meshes/robot_base.stl" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    
    <!-- caster front -->
    <collision name="caster_front_collision">
      <origin rpy=" 0 0 0" xyz="0.35 0 -0.05"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </collision>
    <visual name="caster_front_visual">
      <origin rpy=" 0 0 0" xyz="0.2 0 -0.05"/>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </visual>
  </link>

<link name="link_back_right_wheel">
    <inertial>
      <origin
        xyz="-4.1867E-18 0.0068085 -1.65658661799998E-18"
        rpy="0 0 0" />
      <mass
        value="2" />
      <inertia
        ixx="0.00856502765719703"
        ixy="1.5074118157338E-19"
        ixz="-4.78150098725052E-19"
        iyy="0.013670640432096"
        iyz="-2.68136447099727E-19"
        izz="0.00856502765719703" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="1.5707963267949 0 0" />
      <geometry>
        <mesh filename="package://robot_urdf/meshes/wheel.stl" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="1.5707963267949 0 0" />
      <geometry>
        <mesh filename="package://robot_urdf/meshes/wheel.stl" />
      </geometry>
    </collision>
  </link>


<link name="link_back_left_wheel">
    <inertial>
      <origin
        xyz="-4.1867E-18 0.0068085 -1.65658661799998E-18"
        rpy="0 0 0" />
      <mass
        value="2" />
      <inertia
        ixx="0.00856502765719703"
        ixy="1.5074118157338E-19"
        ixz="-4.78150098725052E-19"
        iyy="0.013670640432096"
